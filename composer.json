{
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "artesaos/seotools": "^0.19.0",
        "coderello/laravel-shared-data": "^3.0",
        "codezero/laravel-localized-routes": "^2.0",
        "codezero/laravel-unique-translation": "^2.0",
        "deployer/deployer": "^6.0",
        "doctrine/dbal": "^2.0",
        "erusev/parsedown": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/validation": "^2.4",
        "laravel-notification-channels/webhook": "^2.1",
        "laravel/fortify": "^1.5",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.0",
        "laravel/telescope": "^4.0",
        "laravel/tinker": "^2.0",
        "okipa/laravel-bootstrap-components": "^3.0",
        "okipa/laravel-brickables": "^2.0",
        "okipa/laravel-medialibrary-ext": "^8.0",
        "okipa/laravel-stuck-jobs-notifier": "^2.0",
        "okipa/laravel-supervisor-downtime-notifier": "^2.0",
        "okipa/laravel-table": "^3.0",
        "plank/laravel-metable": "^3.0",
        "propaganistas/laravel-phone": "^4.0",
        "realrashid/sweet-alert": "^3.0",
        "sentry/sentry-laravel": "^2.0",
        "spatie/laravel-feed": "^3.0",
        "spatie/laravel-honeypot": "^3.0",
        "spatie/laravel-medialibrary": "^8.9",
        "spatie/laravel-sitemap": "^5.0",
        "spatie/laravel-translatable": "^4.0",
        "spatie/laravel-welcome-notification": "^2.0",
        "spatie/pdf-to-image": "^2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "barryvdh/laravel-ide-helper": "^2.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6",
        "phpmd/phpmd": "^2.0",
        "phpunit/phpunit": "^9.3",
        "shipping-docker/vessel": "^5.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan horizon:publish --ansi",
            "@php artisan telescope:publish --ansi",
            "@ide-helper"
        ],
        "ide-helper": [
            "@php artisan ide-helper:eloquent",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:model --dir=app/Models --nowrite",
            "@php artisan ide-helper:meta"
        ],
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd app,config,database,routes text phpmd.xml",
            "vendor/bin/phpstan analyse --error-format=table --memory-limit=2048M",
            "vendor/bin/phpunit --testdox --coverage-text"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
